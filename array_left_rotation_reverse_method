#include <iostream>
#include<bits/stdc++.h>
#include<string.h>
using namespace std;

void reversee(int arr[],int start,int endd)
{
    while(start<endd)
    {
        int temp = arr[start];
        arr[start]=arr[endd];
        arr[endd] = temp;
        start++;
        endd--;
    }
}


int main()
{   //left rotate
    int ar[] = {1,2,3,4,5};
    int d=2;
    reversee(ar,0,1);
    reversee(ar,2,4);
    reversee(ar,0,4);
    for(int i=0;i<5;i++)cout<<ar[i];
    return 0;
}

