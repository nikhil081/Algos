#include <iostream>
#include<bits/stdc++.h>
#include<string.h>
using namespace std;


int main()
{
    string str;
    cin>>str;
    string str1  =str;
    reverse(str.begin(),str.end());
    if(str1==str)
    {
        cout<<"yes";
    }
    else if(str1!=str)
    {
        cout<<"no";
    }
    return 0;
}
