
    int count0=0,count1=0,count2=0;
    for(int i=0;i<n;i++){
        if(a[i]==0){
            count0++;
        }else if(a[i]==1){
            count1++;
        }else if(a[i]==2){
            count2++;
        }
    }
    
    for(int j=0;j<count0;j++) {
        a[j]=0;
    }
    for(int j=count0;j<count0+count1;j++) {
        a[j]=1;
    }
    for(int j=count0+count1;j<n;j++) {
        a[j]=2;
    }
