 vector<int> subarraySum(int arr[], int n, long long s)
    {
         int start=0;
         int end=0;
        long long csum = 0; //current sum
        while(end <= n) {
            while(csum < s) csum += arr[end++];
            if(s == csum) return {start+1,end};
            if(csum > s) csum-=arr[start++];       
        }
    }
