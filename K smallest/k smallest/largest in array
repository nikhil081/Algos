#include <iostream>
#include <bits/stdc++.h>

using namespace std;

int main()
{
    priority_queue<int,vector<int>,greater<int>>heap;
    int arr[]={2,5,3,1,6};
    for(int i=0;i<5;i++){
        heap.push(arr[i]);
    }
    int i=1,res=0;
    while(!heap.empty()){
        if(i==1){
            res=heap.top();
            break;
        }
        heap.pop();
        i++;
    }
    cout<<res;
    return 0;
}

// max heap means descending and implemented as priority_queue<int,vector<int>>heap;
