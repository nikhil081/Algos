

#include<bits/stdc++.h>
#include<string>
using namespace std;

void removeChar(char str[],char c)
{
    int countt=0;
    for(int i=0; i<strlen(str); i++) if(str[i]!=c) str[countt++] = str[i];
    str[countt]='\0';
}
int main()
{
    char s[] = "geeksforgeeks";
    removeChar(s, 'g');
    cout << s;
    return 0;
}



